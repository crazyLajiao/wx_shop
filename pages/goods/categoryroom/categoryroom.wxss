/* pages/goods/categoryroom/categoryroom.wxss */
.topimg{
  width:100%;
  height:996rpx;
  margin-bottom:20rpx;
}
.topimg image{
  width:100%;
  height:100%;
}
.midlist{
  width:828rpx;
  display:flex;
  flex-direction: column;
  align-items: middle;
  text-align: center;
  margin-bottom:50rpx;
}
.midlist .title{
  width:100%;
  height: 110rpx;
  margin-bottom:36rpx;
}
.midlist .title image{
  width:100%;
  height:100%;
}
.midlist .list-box{
  width:100%;
  height:500rpx;
  display: flex;
  justify-content: space-between;
  flex-flow: wrap;
}
.midlist .list-item{
  position: relative;
  width:30%;
  height:245rpx;
  margin-bottom:26rpx;
} 
.midlist .item-img{
  width:100%;
  height:100%;
}
.midlist .item-name{
  position: absolute;
  display: inline-block;
  width:100%;
  height:45rpx;
  line-height: 45rpx;
  text-align: center;
  background-color: rgba(255,255,255,.6);
  font-size:32rpx;
  left:0;
  bottom:0rpx;
  z-index:99;
}
.list-container{
  display: flex;
  flex-direction: column;
  width: 100%;
}
.list-nav{
  width:100%;
  height:240rpx;
   padding-left:19rpx;
  padding-right:12rpx;
  padding-bottom:5rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
}
.nav-item{
  width:21%;
  height:100%;
  margin:10rpx auto;
  color:#666;
}
.active{
  color:#000;
  font-weight: 700;
  border-bottom: 8rpx solid #e64340;
}
.nav-img{
  width:100%;
  height:75%;
  text-align: center;
}
.nav-img image{
  width:100%;
  height:100%;
}
.nav-name{
  width:100%;
  height:23%;
  font-size:28rpx;
  text-align: center;
  margin-bottom:0;
}
.list-content{
  width: 100%;
  margin-top:36rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}
.content-item{
  width:48%;
  height:600rpx;
  text-align: center;
  box-sizing: border-box;
  padding:2rpx 20rpx;
  margin:10rpx 0;
}
.content-item .item-img{
  width:100%;
  height:440rpx;
}
.content-item .item-img image{
  width:100%;
  height:100%;
}
.content-item .item-desc{
  margin-top:20rpx;
  font-size:24rpx;
  width:100%;
  text-align: center;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.content-item .item-price{
  width:100%;
}
.item-price .price{
  font-size:32rpx;
  font-weight: 400;
}
.item-price .old-price{
  color:#98989f;
  font-size:24rpx;
  margin-left:5rpx;
}
.content-item button{
  width: 80%;
  height:60rpx;
  line-height: 60rpx;
  text-align: center;
  background-color: #e64340;
  color:#fff;
  font-size:30rpx;
}
.list-brand{
  width:100%;
  display: flex;
  flex-direction: column;
}
.brand-item{
  width:100%;
  height:580rpx;
  margin-top:20rpx;
  position: relative;
}
.brand-img{
  position: absolute;
  width:100%;
  height:470rpx;
}
.brand-img image{
  width:100%;
  height:100%;
}
.brand-desc{
  position: absolute;
  left:10%;
  bottom:50rpx;
  width:70%;
  font-size:42rpx;
  margin-top:20rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}